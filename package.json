    "react-redux": "^5.0.7",
    "recompose": "^0.27.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "1.0.0-alpha.2",
    "reselect": "^3.0.1",
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "app-icon": "^0.6.3",
    "axios-mock-adapter": "^1.15.0",
